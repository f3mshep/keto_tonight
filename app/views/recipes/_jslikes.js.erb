class Like{
    constructor(){
        this.likeButton = document.getElementsByName("edit_like")[0]
        this.userId = currentUserId
        this.recipeId = currentRecipeId
        this.status = this.likeButton.innerHTML
    }

    switchStatus(){
        if(this.status ==="Like"){
            this.status ==="Unlike"
        }
        else{
            this.status === "Like"
        }
    }

    
}